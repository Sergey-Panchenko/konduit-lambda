{
  "name": "konduit-server",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "start-pm2": "pm2 kill && pm2 start kme-pm2.config.js --env development",
    "stop-pm2": "pm2 stop kme-pm2.config.js",
    "prestart": "mkdir -p logs",
    "start": "pm2 kill && pm2-docker start kme-pm2.config.js â€“raw",
    "start-dev": "NODE_ENV=test DEBUG=konduit-server:* nodemon ./index.js",
    "start-debug": "NODE_ENV=test DEBUG=konduit-server:* node --inspect --debug-brk ./index.js",
    "lint": "eslint ./index.js ./src ./test",
    "test": "NODE_ENV=test mocha \"test/**/*.test.js\" -c",
    "integration-test": "NODE_ENV=test mocha \"integration-test/**/*.test.js\" -c --timeout 50000",
    "cover": "NODE_ENV=test istanbul cover --report html --report text node_modules/.bin/_mocha \"test/**/*.test.js\" -c"
  },
  "eslintConfig": "./.eslintrc.json",
  "engines": {
    "node": "20",
    "npm": "10"
  },
  "dependencies": {
    "aws-sdk": "^2.1509.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie": "^0.6.0",
    "cookie-parser": "^1.4.6",
    "data-urls": "^5.0.0",
    "debug": "~4.3.4",
    "entities": "^4.5.0",
    "express": "~4.18.2",
    "hot-shots": "^10.0.0",
    "libxmljs": "1.0.11",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "pm2": "5.3.0",
    "request": "^2.88.2",
    "serverless-http": "^3.2.0",
    "toobusy-js": "^0.5.1",
    "tough-cookie": "^4.1.3",
    "ua-parser-js": "^1.0.37",
    "url-parse": "^1.5.10",
    "whatwg-encoding": "^3.1.1",
    "winston": "^2.4.7",
    "winston-console-formatter": "0.3.1",
    "winston-logstash": "^1.2.1"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-react": "^7.33.2",
    "expect": "^1.20.2",
    "istanbul": "^0.4.5",
    "mocha": "^10.2.0",
    "nock": "^13.4.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
